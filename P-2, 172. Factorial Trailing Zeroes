/*
Problem: 172. Factorial Trailing Zeroes
Solution Language: C++
Time Complexity: O(logâ‚… n)
Space Complexity: O(1)

Logic: n/5+n/25+n/125+...n/((5)^k)
*/

class Solution {
public:
    int trailingZeroes(int n) {
        int counter=0;
        for(int i=5; n/i>=1;i*=5){
        counter+= n/i;
    }
    return counter;
    }
};
